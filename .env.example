# GIGAVIBE Environment Variables

# Database
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Farcaster Integration
NEYNAR_API_KEY=your_neynar_api_key
NEXT_PUBLIC_NEYNAR_API_KEY=your_neynar_api_key

# IPFS Storage (Pinata) - REQUIRED for upload system
PINATA_API_KEY=your_pinata_api_key
PINATA_SECRET_API_KEY=your_pinata_secret_api_key

# Zora Integration
ZORA_API_KEY=your_zora_api_key

# OnchainKit
NEXT_PUBLIC_ONCHAINKIT_API_KEY=your_onchainkit_api_key
NEXT_PUBLIC_ONCHAINKIT_PROJECT_NAME=GIGAVIBE

# App Configuration
NEXT_PUBLIC_URL=http://localhost:3000
NEXT_PUBLIC_APP_HERO_IMAGE=/images/gigavibeclouds.png
NEXT_PUBLIC_SPLASH_IMAGE=/images/gigavibeclouds.png
NEXT_PUBLIC_SPLASH_BACKGROUND_COLOR=#4b0082

# Optional: FilCDN (Filecoin storage)
FILECOIN_PRIVATE_KEY=your_filecoin_private_key

# AI Services (Optional)
OPENAI_API_KEY=your_openai_api_key
ANTHROPIC_API_KEY=your_anthropic_api_key
GOOGLE_AI_API_KEY=your_google_ai_api_key